{
  "expo": {
    "name": "KmerServices",
    "slug": "kmerservices",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {}
    },
    "scheme": "kmerservices",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kmerservices.app",
      "googleServicesFile": "./GoogleService-Info.plist",
      "associatedDomains": [
        "applinks:kmerservices.com"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kmerservices.app",
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "kmerservices"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ]
    ],
    "notification": {
      "color": "#2D2D2D",
      "androidMode": "default",
      "androidCollapsedTitle": "KmerServices"
    }
  }
}